{
  "name": "bytes-browser",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-dev.sh",
    "build": "bash run-build.sh",
    "dev:mac": "export REACT_APP_VERSION=$(git rev-parse HEAD) && concurrently \"vite\" \"tauri dev\"",
    "dev:win": "for /f %i in ('git rev-parse HEAD') do set REACT_APP_VERSION=%i && concurrently \"vite\" \"tauri dev\"",
    "build:mac": "export REACT_APP_VERSION=$(git rev-parse HEAD) && tsc && vite build",
    "build:win": "for /f %i in ('git rev-parse HEAD') do set REACT_APP_VERSION=%i && tsc && vite build",
    "codegen:build": "graphql-codegen --config codegen.yml",
    "tauri": "tauri"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@tauri-apps/api": "^1.4.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.15",
    "axios": "^1.4.0",
    "eventemitter3": "^5.0.1",
    "feeder-react-feedback": "^0.0.6",
    "graphql": "^16.8.0",
    "history": "^5.3.0",
    "moment": "^2.29.4",
    "postcss": "^8.4.28",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-contexify": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-loader-spinner": "^5.3.4",
    "react-modal": "^3.16.1",
    "react-moment": "^1.1.3",
    "react-router-dom": "^6.15.0",
    "react-select": "^5.7.4",
    "react-toggle": "^4.1.3",
    "react-tooltip": "^5.21.1",
    "recoil": "^0.7.7",
    "recoil-nexus": "^0.5.0",
    "sass": "^1.66.0",
    "scss": "^0.2.4",
    "tauri-plugin-store-api": "^0.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@tauri-apps/cli": "^1.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
